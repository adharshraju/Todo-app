{
    "name": "n-app-todo",
    "version": "0.0.1",
    "description": "n app todo app",
    "packageManager": "yarn@3.3.1",
    "main": "src/server/server.js",
    "scripts": {
        "ts-compile": "tsc -p tsconfig.json",
        "ts-lint": "node --max_old_space_size=6144 ./node_modules/eslint/bin/eslint.js  . --ext .ts",
        "ts-compile-server": "tsc -p tsconfig.server.json",
        "ts-compile-server-watch": "yarn ts-compile-server -w",
        "ts-compile-client": "tsc -p tsconfig.client.json",
        "ts-build": "yarn ts-compile-server && yarn ts-compile-client && yarn ts-lint",
        "webpack-build": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js",
        "webpack-build-watch": "yarn webpack-build -- -w",
        "static-build": "yarn ts-build && node ./compress-static.js",
        "nodemon": "nodemon --ignore test/ --ignore src/client/ --ignore src/sdk/ --inspect=6101 --require source-map-support/register ./src/server/server.js package.name=web-app",
        "dev": "yarn ts-build && concurrently \"yarn ts-compile-server-watch\" \"yarn nodemon\"",
        "test": "yarn ts-build && mocha --timeout 200000 --reporter spec --ui tdd --require ts-node/register \"./test/**/*.test.js\" || true",
        "test-ci": "yarn ts-build && mocha --reporter spec --ui tdd --require ts-node/register \"./test/**/*.test.js\"",
        "clean-src": "find ./src -type f -name \"*.js\" -not -path \"*/3rd-party/*\" -delete -o -name \"*.map\" -not -path \"*/3rd-party/*\" -delete -o -name \".DS_Store\" -delete && rm -f *.tsbuildinfo",
        "clean-test": "find ./test -name \"*.js\" -delete -o -name \"*.map\" -delete && rm -f *.tsbuildinfo",
        "build": "yarn static-build && env=prod yarn webpack-build",
        "start": "node ./src/server/server.js package.name=web-app"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shreymahendru/n-app-todo.git"
    },
    "keywords": [
        "n-app",
        "todo",
        "web",
        "app"
    ],
    "author": "Shrey Mahendru",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/shreymahendru/n-app-todo/issues"
    },
    "homepage": "https://github.com/SofDevInc/n-app-todo#readme",
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/preset-env": "^7.18.6",
        "@types/mocha": "^9.1.1",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "btoa": "^1.2.1",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^10.0.0",
        "concurrently": "^7.2.2",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "enhanced-resolve": "^5.9.3",
        "eslint": "^8.19.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^7.2.11",
        "hash-sum": "^2.0.0",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "html-webpack-tags-plugin": "^3.0.2",
        "imagemin": "^7.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-svgo": "^9.0.0",
        "imagemin-webp": "^6.0.0",
        "loader-utils": "^2.0.0",
        "memfs": "^3.4.4",
        "mini-css-extract-plugin": "^2.6.0",
        "mocha": "^10.0.0",
        "moment-locales-webpack-plugin": "^1.2.0",
        "node-sass": "^7.0.1",
        "nodemon": "^2.0.16",
        "postcss": "^8.4.14",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^7.0.0",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.52.1",
